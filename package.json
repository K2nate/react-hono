{
  "name": "react-hono-cloudflare",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "check-types": "turbo check-types",
    "biome:check": "biome check .",
    "biome:fix": "biome check . --write",
    "test": "turbo run test",
    "test:projects": "vitest run",
    "test:projects:watch": "vitest --watch",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.14.0",
  "devDependencies": {
    "@biomejs/biome": "2.1.4",
    "@repo/vitest-config": "workspace:*",
    "husky": "^8.0.0",
    "lint-staged": "^16.1.5",
    "turbo": "^2.5.5",
    "vitest": "^3.2.4"
  },
  "lint-staged": {
    "apps/**/src/**/*.{js,jsx,ts,tsx,json}": [
      "pnpm biome:fix"
    ],
    "packages/**/*.{js,jsx,ts,tsx,json}": [
      "pnpm biome:fix"
    ],
    "docs/**/*.json": [
      "pnpm biome:fix"
    ],
    "{biome,package,turbo}.json": [
      "pnpm biome:fix"
    ]
  }
}
